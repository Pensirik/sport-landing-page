{
  "extends": "oci-react",
  "ignorePatterns": ["", "**/vendor/*.js"],
  "rules": {
    "object-curly-newline": ["warn", {
      "ImportDeclaration": { "multiline": true, "minProperties": 6 }
    }],
    "global-require": "off",
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": "error",
    "new-cap": "off"
  },
  "globals": {
    "Swiper":true,
    "window": true,
    "document": true
  }
}

